    {
      "alias":     "staging",
      "hostname":  "staging.server.com",
      "username":  "user",
      "password":  "password",
      "directory": "/srv/myapp"
    },
    {
      "alias":     "production",
      "hostname":  "production.server.com",
      "username":  "user",
      "password":  "password",
      "directory": "/srv/myapp"
    }
