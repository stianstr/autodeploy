    "url":     "git@git.server.com:repos/repos.git",
    "workdir": "/srv/autodeploy/data/work",
    "sshkey":  "/srv/autodeploy/data/ssh/id_dsa"
  },
  "result": {
    "datadir": "/srv/autodeploy/data/log"
  },
